{
  "@name": "贴吧内容屏蔽器",
  "alias": "mkTieBaContentShield",
  "@namespace": "http://tampermonkey.net/",
  "@license": "GPL-3.0",
  "@version": "1.1",
  "@author": "byhgz",
  "@description": "对贴吧(tieba.baidu.com)部分页面内容进行屏蔽处理",
  "@icon": "https://tb3.bdstatic.com/public/icon/favicon-v2.ico",
  "noFrames": true,
  "@run-at": "document-start",
  "@grant": [
    "GM_setValue",
    "GM_getValue",
    "GM_deleteValue",
    "GM_addStyle",
    "GM_registerMenuCommand"
  ],
  "@exclude": [
    "http://localhost:3001/"
  ],
  "@match": [
    "*://localhost/*",
    "*://tieba.baidu.com/*"
  ],
  "@require": [
    "https://unpkg.com/vue@2.7.16/dist/vue.min.js",
    "https://unpkg.com/element-ui@2.15.14/lib/index.js"
  ]
}

